<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Tabela de Funcion√°rios</title>
</head>
<body>
    <table>
        <thead>
          <tr>
            <th>ID</th>
            <th>Nome</th>
            <th>Horas Trabalhadas</th>
            <th>Turno de Trabalho</th>
            <th>Categoria</th>
          </tr>
        </thead>
        <tbody>
            <% data.forEach(funcionario => { %>
              <tr>
                <td><%= funcionario.id %></td>
                <td><%= funcionario.nomeFuncionario %></td>
                <td><%= funcionario.horasTrabalhadas %></td>
                <td><%= funcionario.turnoTrabalho %></td>
                <td><%= funcionario.categoria %></td>
              </tr>
            <% }); %>
        </tbody>
      </table>      
</body>
</html>
